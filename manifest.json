{
  "manifest_version": 2,
  "name": "History Access Extension",
  "version": "1.0",
  "description": "An extension to display browsing history on an extension page.",
  "permissions": ["history", "tabs", "storage"],
  "browser_action": {
    "default_title": "History Access",
    "default_icon": {
      "48": "icons/browser_history_icon_48x48.png",
      "96": "icons/browser_history_icon_96x96.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
}
